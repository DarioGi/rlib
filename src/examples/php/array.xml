<?xml version="1.0"?>
<!DOCTYPE report >
<Report fontSize="20" orientation="landscape">
	<Alternate>
		<NoData>
			<Output>
				<Line fontSize="12">
					<literal>NO DATA</literal>
				</Line>		
			</Output>
		</NoData>
	</Alternate>
	
	<ReportHeader>
		<Output>
			<Line fontSize="11">
				<field value="2" format="'%02.03d'" width="10"/>
				<literal>Test Report (Normal) AND THE REPORT FORMAT IS: </literal>
				<field value="r.format" width="10"/>
			</Line>
			<Line fontSize="11">
				<literal bold="yes">Test Report (Bold)</literal>
			</Line>
			<Line fontSize="11">
				<literal italics="yes">Test Report (Italics)</literal>
			</Line>
			<Line fontSize="11">
				<literal bold="yes" italics="yes">Test Report (Bold and Italics)</literal>
			</Line>
			<Line fontSize="11">
				<literal bold="yes" italics="yes">Whee: </literal>
				<field format="'%.5d'" value="abs(-5.3333)" width="10"/>
			</Line>
			<Line fontSize="11">
				<literal bold="yes" italics="yes">BOO: </literal>
				<field value="'mike is a silly pants'" width="10"/>
			</Line>
			<Line fontSize="11">
				<literal bold="yes" italics="yes">TESTING CUSTOM FUNCTION </literal>
				<field value="bobdoan(mikeroth('bacon', 'eggs'))" width="50"/>
			</Line>
			<HorizontalLine size="4" bgcolor="'white'"/>
		</Output>
	</ReportHeader>		
	<Variables>
		<Variable name="oscar" value="2+3" type="expression"/>
		<Variable name="precal" value="val(group)" type="sum" resetonbreak="break0" precalculate="yes"/>
		<Variable name="edude" value="val(group)" type="sum" precalculate="yes"/>
		<Variable name="actual" value="val(group)" type="sum" resetonbreak="break0"/>
	</Variables>
	<Breaks>	
		<Break name="break0" newpage="no" headernewpage="yes">
			<BreakHeader>
				<Output>
					<Line>
						<field value="val(group)" width="20" format="'%.2d'" align="left" col="1"/>					
						<literal>TEST::: </literal>
						<field value="v.precal" width="10"/>
						<literal>EDUDE::: </literal>
						<field value="v.edude" width="10"/>
					</Line>
				</Output>								
			</BreakHeader>
			<BreakFields>
				<BreakField value="group"/>
			</BreakFields>
			<BreakFooter>
				<Output>
					<HorizontalLine size="4" bgcolor="'white'"/>
					<HorizontalLine size="2" bgcolor="'black'"/>
					<HorizontalLine size="4" bgcolor="'white'"/>
					<Line>
						<field value="group" width="20" align="left" col="1"/>					
						<literal>WE NEED TO GET:::</literal>
						<field value="v.actual" width="10"/>
					</Line>
					<HorizontalLine size="4" bgcolor="'white'"/>
					<HorizontalLine size="2" bgcolor="'black'"/>
					<HorizontalLine size="4" bgcolor="'white'"/>
				</Output>
			</BreakFooter>
		</Break>
	</Breaks>
	<Detail>
		<FieldHeaders>
			<Output>
				<HorizontalLine size="1" bgcolor="'black'"/>
				<Line bgcolor="'0xe5e5e5'">
					<literal width="20" col="1">First Name</literal>
					<literal width="1"/>
					<literal width="20" col="2">Last Name</literal>
					<literal width="1"/>
					<literal width="20" col="3">Color</literal>
				</Line>
				<HorizontalLine size="1" length="v.oscar" bgcolor="'black'"/>
				<HorizontalLine size="4" bgcolor="'white'"/>
			</Output>
		</FieldHeaders>		
		<FieldDetails>
			<Output>
				<Line bgcolor="iif(r.detailcnt%2,'0xe5e5e5','white')">
					<field value="first_name" width="20" align="left" col="1" bgcolor="'yellow'" link="'http://www.yahoo.com'"/>
					<field value="last_name" width="20" align="left" col="2" bgcolor="'yellow'" link="'http://www.yahoo.com'"/>
					<literal width="1"/>
					<field value="color" width="20" align="left" col="3"/>
				</Line>
			</Output>
		</FieldDetails>
	</Detail>
	<PageFooter>
		<Output>
			<Line>
				<literal>Page: </literal>	
				<field value="r.pageno" width="3" align="right"/>
			</Line>
		</Output>
	</PageFooter>

	<ReportFooter>
	</ReportFooter>
</Report>
