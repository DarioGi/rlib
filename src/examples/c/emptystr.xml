<?xml version="1.0"?>
<!DOCTYPE report >
<Report fontSize="9" orientation="portrait">
	<ReportHeader>
		<Output>
			<Line/>
			<Line fontSize="12">
				<literal>Empty string from memory variable:</literal>
				<field value="m.param" />
			</Line>
			<Line fontsize="4"/>
			<HorizontalLine size="4" bgcolor="'white'"/>
			<HorizontalLine size="2" bgcolor="'black'"/>
			<HorizontalLine size="4" bgcolor="'white'"/>
