				</Line>
				<HorizontalLine size="1" bgcolor="'black'"/>
				<HorizontalLine size="4" bgcolor="'white'"/>
			</Output>
		</FieldHeaders>
		<FieldDetails>
			<Output>
				<Line bgcolor="iif(r.detailcnt%2,'0xe5e5e5','white')">
					<field value="data.field" width="15" align="left" col="1"/>
				</Line>
				<HorizontalLine size="1" bgcolor="'black'"/>
				<HorizontalLine size="4" bgcolor="'white'"/>
			</Output>
		</FieldDetails>
	</Detail>
</Report>
